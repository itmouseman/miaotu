<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:zhy="http://schemas.android.com/tools"
    android:id="@+id/rl_content_sx"
    android:layout_width="310dp"
    android:layout_height="match_parent"
    android:background="@color/white_color"
    android:orientation="vertical">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginBottom="200dp"
            android:background="@color/white_color"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginTop="35dp"
                android:gravity="center_vertical">

                <ImageView
                    android:id="@+id/iv_shaixuanBack"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:padding="4dp"
                    android:src="@drawable/ic_baseline_arrow_back_ios_24"></ImageView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="苗木名称"
                    android:textColor="#333333"
                    android:textSize="15sp"></TextView>
            </LinearLayout>

            <com.widget.miaotu.common.utils.home.EditViewWithDel
                android:id="@+id/edView_shaixuan_pop"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_marginTop="10dp"
                android:background="#F2F2F2"
                android:hint="  请输入苗木名称"
                android:paddingLeft="10dp"
                android:textSize="14sp" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="10dp"
                android:background="#EEEEEE"></View>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="规格(厘米)"
                    android:textColor="#333333"
                    android:textSize="15sp"></TextView>

                <TextView
                    android:id="@+id/tv_go_shoudong_qujian"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:text="手动设置区间"
                    android:textColor="#00B0A0"
                    android:textSize="12sp"
                    android:visibility="gone"></TextView>

                <TextView
                    android:id="@+id/tv_go_huadong_qujian"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:text="滑动设置区间"
                    android:textColor="#00B0A0"
                    android:textSize="12sp"></TextView>
            </RelativeLayout>

            <!--SeekBar-->
            <LinearLayout
                android:id="@+id/ll_guiGe_seekBar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:orientation="vertical">


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_alignParentBottom="true"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="杆径"
                        android:textColor="#666666"
                        android:textSize="@dimen/textSize_12"></TextView>

                    <RelativeLayout
                        android:layout_width="230dp"
                        android:layout_height="60dp"
                        android:layout_alignParentRight="true">

                        <TextView
                            android:id="@+id/tv_seekBar1_start"
                            android:layout_width="wrap_content"
                            android:layout_height="20dp"
                            android:layout_marginLeft="10dp"
                            android:gravity="center_vertical"
                            android:text="0"
                            android:textColor="@color/colorAccent"
                            android:textSize="12sp"></TextView>

                        <TextView
                            android:id="@+id/tv_seekBar1_end"
                            android:layout_width="wrap_content"
                            android:layout_height="20dp"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="10dp"
                            android:gravity="center_vertical"
                            android:text="50"
                            android:textColor="@color/colorAccent"
                            android:textSize="12sp"></TextView>

                        <com.jaygoo.widget.RangeSeekBar
                            android:id="@+id/sb_range_1"
                            android:layout_width="230dp"
                            android:layout_height="40dp"
                            android:layout_alignParentBottom="true"
                            app:rsb_gravity="center"
                            app:rsb_max="50"
                            app:rsb_mode="range"
                            app:rsb_progress_color="@color/colorAccent"
                            app:rsb_thumb_drawable="@mipmap/iv_seekbar"
                            app:rsb_thumb_inactivated_drawable="@mipmap/iv_seekbar" />
                    </RelativeLayout>


                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:orientation="vertical">


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_alignParentBottom="true"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="高度"
                        android:textColor="#666666"
                        android:textSize="@dimen/textSize_12"></TextView>

                    <RelativeLayout
                        android:layout_width="230dp"
                        android:layout_height="match_parent"
                        android:layout_alignParentRight="true">

                        <TextView
                            android:id="@+id/tv_seekBar2_start"
                            android:layout_width="wrap_content"
                            android:layout_height="20dp"
                            android:layout_marginLeft="10dp"
                            android:gravity="center_vertical"
                            android:text="0"
                            android:textColor="@color/colorAccent"
                            android:textSize="12sp"></TextView>

                        <TextView
                            android:id="@+id/tv_seekBar2_end"
                            android:layout_width="wrap_content"
                            android:layout_height="20dp"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="10dp"
                            android:gravity="center_vertical"
                            android:text="2000"
                            android:textColor="@color/colorAccent"
                            android:textSize="12sp"></TextView>

                        <com.jaygoo.widget.RangeSeekBar
                            android:id="@+id/sb_range_2"
                            android:layout_width="230dp"
                            android:layout_height="40dp"
                            android:layout_alignParentBottom="true"
                            app:rsb_gravity="center"
                            app:rsb_max="2000"
                            app:rsb_mode="range"
                            app:rsb_progress_color="@color/colorAccent"

                            app:rsb_thumb_drawable="@mipmap/iv_seekbar"
                            app:rsb_thumb_inactivated_drawable="@mipmap/iv_seekbar" />
                    </RelativeLayout>


                </RelativeLayout>


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_alignParentBottom="true"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="冠幅"
                        android:textColor="#666666"
                        android:textSize="@dimen/textSize_12"></TextView>

                    <RelativeLayout
                        android:layout_width="230dp"
                        android:layout_height="match_parent"
                        android:layout_alignParentRight="true">

                        <TextView
                            android:id="@+id/tv_seekBar3_start"
                            android:layout_width="wrap_content"
                            android:layout_height="20dp"
                            android:layout_marginLeft="10dp"
                            android:gravity="center_vertical"
                            android:text="0"
                            android:textColor="@color/colorAccent"
                            android:textSize="12sp"></TextView>

                        <TextView
                            android:id="@+id/tv_seekBar3_end"
                            android:layout_width="wrap_content"
                            android:layout_height="20dp"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="10dp"
                            android:gravity="center_vertical"
                            android:text="5000"
                            android:textColor="@color/colorAccent"
                            android:textSize="12sp"></TextView>

                        <com.jaygoo.widget.RangeSeekBar
                            android:id="@+id/sb_range_3"
                            android:layout_width="230dp"
                            android:layout_height="40dp"
                            android:layout_alignParentBottom="true"
                            app:rsb_gravity="center"
                            app:rsb_max="5000"
                            app:rsb_mode="range"
                            app:rsb_progress_color="@color/colorAccent"

                            app:rsb_thumb_drawable="@mipmap/iv_seekbar"
                            app:rsb_thumb_inactivated_drawable="@mipmap/iv_seekbar" />
                    </RelativeLayout>


                </RelativeLayout>
            </LinearLayout>

            <!--文本-->
            <LinearLayout
                android:id="@+id/ll_guiGe_wenBen"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="杆径"
                        android:textColor="#666666"
                        android:textSize="12sp"></TextView>

                    <EditText
                        android:id="@+id/et_gj_start"
                        android:layout_width="85dp"
                        android:layout_height="28dp"
                        android:layout_marginLeft="15dp"
                        android:background="@color/base_gray"
                        android:gravity="center"
                        android:hint="@string/text_ShuruShuZhi"
                        android:digits="1234567890"
                        android:textSize="@dimen/textSize_12"></EditText>

                    <View
                        android:layout_width="9dp"
                        android:layout_height="1dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/base_gray"></View>

                    <EditText
                        android:id="@+id/et_gj_end"
                        android:layout_width="85dp"
                        android:layout_height="28dp"
                        android:background="@color/base_gray"
                        android:gravity="center"
                        android:hint="@string/text_ShuruShuZhi"
                        android:digits="1234567890"
                        android:textSize="@dimen/textSize_12"></EditText>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_weight="1"
                        android:gravity="right"
                        android:text="厘米"
                        android:textColor="#666666"

                        android:textSize="@dimen/textSize_12"></TextView>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="高度"
                        android:textColor="#666666"
                        android:textSize="12sp"></TextView>

                    <EditText
                        android:id="@+id/et_gd_start"
                        android:layout_width="85dp"
                        android:layout_height="28dp"
                        android:layout_marginLeft="15dp"
                        android:background="@color/base_gray"
                        android:gravity="center"
                        android:hint="@string/text_ShuruShuZhi"
                        android:digits="1234567890"
                        android:textSize="@dimen/textSize_12"></EditText>

                    <View
                        android:layout_width="9dp"
                        android:layout_height="1dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/base_gray"></View>

                    <EditText
                        android:id="@+id/et_gd_end"
                        android:layout_width="85dp"
                        android:layout_height="28dp"
                        android:background="@color/base_gray"
                        android:gravity="center"
                        android:hint="@string/text_ShuruShuZhi"
                        android:digits="1234567890"
                        android:textSize="@dimen/textSize_12"></EditText>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_weight="1"
                        android:gravity="right"
                        android:text="厘米"
                        android:textColor="#666666"

                        android:textSize="@dimen/textSize_12"></TextView>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="冠幅"
                        android:textColor="#666666"
                        android:textSize="12sp"></TextView>

                    <EditText
                        android:id="@+id/et_gf_start"
                        android:layout_width="85dp"
                        android:layout_height="28dp"
                        android:layout_marginLeft="15dp"
                        android:background="@color/base_gray"
                        android:gravity="center"
                        android:hint="@string/text_ShuruShuZhi"
                        android:digits="1234567890"
                        android:textSize="@dimen/textSize_12"></EditText>

                    <View
                        android:layout_width="9dp"
                        android:layout_height="1dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/base_gray"></View>

                    <EditText
                        android:id="@+id/et_gf_end"
                        android:layout_width="85dp"
                        android:layout_height="28dp"
                        android:background="@color/base_gray"
                        android:gravity="center"
                        android:hint="@string/text_ShuruShuZhi"
                        android:digits="1234567890"
                        android:textSize="@dimen/textSize_12"></EditText>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_weight="1"
                        android:gravity="right"
                        android:text="厘米"
                        android:textColor="#666666"

                        android:textSize="@dimen/textSize_12"></TextView>
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="10dp"
                android:background="#EEEEEE"></View>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="地区"
                    android:textColor="#333333"
                    android:textSize="@dimen/textSize_14"></TextView>


                <TextView
                    android:id="@+id/tv_right_dq1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/tv_right_dq"
                    android:gravity="center"
                    android:text="不限"
                    android:textColor="#333333"
                    android:textSize="@dimen/textSize_14"></TextView>

                <ImageView
                    android:id="@+id/tv_right_dq"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/ic_baseline_chevron_right_24"></ImageView>

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="10dp"
                android:background="#EEEEEE"></View>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="18dp"
                android:text="标签"
                android:textColor="#333333"
                android:textSize="@dimen/textSize_14"></TextView>


            <com.zhy.view.flowlayout.TagFlowLayout
                android:id="@+id/id_flow_biaoQian"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                zhy:max_select="1"></com.zhy.view.flowlayout.TagFlowLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="18dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:text="类型"
                    android:textColor="#333333"
                    android:textSize="@dimen/textSize_14"></TextView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="(可多选)"
                    android:textColor="#777777"
                    android:textSize="@dimen/textSize_12"></TextView>
            </LinearLayout>


            <com.zhy.view.flowlayout.TagFlowLayout
                android:id="@+id/id_flow_leiXing"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                zhy:max_select="1"></com.zhy.view.flowlayout.TagFlowLayout>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="18dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:text="企业认证"
                    android:textColor="#333333"
                    android:textSize="@dimen/textSize_14"></TextView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="(可多选)"
                    android:textColor="#777777"
                    android:textSize="@dimen/textSize_12"></TextView>
            </LinearLayout>


            <com.zhy.view.flowlayout.TagFlowLayout
                android:id="@+id/id_flow_QiyeRenzheng"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                zhy:max_select="1"></com.zhy.view.flowlayout.TagFlowLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_gravity="bottom"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_Cance_Sure"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/white_color"
                android:gravity="center"
                android:text="重置"
                android:textColor="#333333"
                android:textSize="18sp"></TextView>

            <TextView
                android:id="@+id/tv_ShaiXuan_Sure"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/colorAccent"
                android:gravity="center"
                android:text="完成"
                android:textColor="@color/white_color"
                android:textSize="18sp"></TextView>
        </LinearLayout>
    </FrameLayout>

</androidx.core.widget.NestedScrollView>

